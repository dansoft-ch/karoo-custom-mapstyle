    <!-- place labels -->
	<!-- modifiziert by Dänu: Städenamen nicht kursiv; So, dass Bern in jeder Zoomstufe angezeigt wird; Text etwas grösser; Schwarz -->
	<!-- ACHTUNG: je höher die Zahl der Priorität, desto gewichtiger ist die Anzeige. Also Städte hohe Prio, Quartiere tiefe Prio -->
	<!-- Das mit den Prioritäten müsste mal einer den Entwicklern sagen :-) -->
    <m e="node" k="place">
		<m v="city" zoom-min="6">
            <caption style="bold" fill="#000000" k="name" priority="70" size="20"
                    font-family="thin" stroke="#ffffff" stroke-width="2.0" />
        </m>
		<m v="town" zoom-min="7">
            <caption style="bold" fill="#000000" k="name" priority="50" size="18"
                    font-family="thin" stroke="#ffffff" stroke-width="2.0" />
